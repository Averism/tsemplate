{
  "name": "averframework",
  "version": "0.0.1",
  "description": "Basic Framework for Averism",
  "scripts": {
    "start": "rubah generate && node -r ts-node/register src/index.ts",
    "build": "rm -rf build && mkdir build && npm run build:ts && npm run build:ts.d",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:ts.d": "tsc -d --project tsconfig.build.json --emitDeclarationOnly",
    "preprocess": "rubah generate",
    "revertTemplate": "rubah revert"
  },
  "author": "Averman",
  "license": "GPL-3.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": "bin",
  "dependencies": {
    "express": "^4.17.1",
    "rubah": "github:averism/rubah",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.11.8",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "averModule": {
    "rubah": {
      "config": ".avermodule/rubah/rubahconfig.json",
      "firstrun": true
    }
  }
}
